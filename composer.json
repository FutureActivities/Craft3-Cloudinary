{
  "name": "futureactivities/craft3-cloudinary",
  "description": "Cloudinary volume for Craft 3",
  "type": "craft-plugin",
  "version": "0.8.0",
  "keywords": [
    "cloudinary",
    "cms",
    "craftcms",
    "flysystem",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Future Activities",
      "homepage": "https://github.com/FutureActivities"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0-beta.24",
    "enl/flysystem-cloudinary": "^1.0",
    "cloudinary/cloudinary_php": "~1.8"
  },
  "autoload": {
    "psr-4": {
      "futureactivities\\cloudinary\\": "src/"
    }
  },
  "extra": {
    "name": "Cloudinary",
    "handle": "cloudinary",
    "components": {
      "transform": "futureactivities\\cloudinary\\services\\Transform"
    }
  }
}
